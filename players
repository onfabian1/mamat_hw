        var adWrap = document.querySelector('.js-ad-takeover');
        var slotId = '/131332370/GeneralTakeover_0';

        googletag.cmd.push(function() { 

            var mapping = googletag.sizeMapping(). 
            addSize([0, 0], []). 
            addSize([200, 300], [[320, 50], [320, 480]]).
            addSize([450, 300], [[320, 50], [1600, 900]]).      
            addSize([750, 300], [[728, 90], [1600, 900]]). 
            addSize([990, 300], [[1600, 900], [970, 250], [728, 90]]).build();

            googletag.defineSlot('/131332370/GeneralTakeover', [
                [1600, 900], 
                [320, 480]
            ], 'div-gpt-ad-1503491171981-0').
                defineSizeMapping(mapping).
                addService(googletag.pubads());
            googletag.pubads().enableSingleRequest();
            googletag.enableServices();

            googletag.pubads().addEventListener('slotRenderEnded', function (event) {
            
                if ( slotId === event.slot.getSlotId().toString() ) {
                    if (event.isEmpty) {
                        adWrap.style.display = 'none';
                    } else {
                        adWrap.style.display = 'block';
                    }
                }
            });

            googletag.display('div-gpt-ad-1503491171981-0');

        });

        // PULSELIVE SCRIPT
        var closeBtn = document.getElementById('advertClose');
        var closeLogo = document.getElementById('logoClose');

        closeBtn.addEventListener('click', function( e ){
            e.preventDefault();
            adWrap.innerHTML = "";
            adWrap.style.width = 0;
            adWrap.style.height = 0;
        });

        closeLogo.addEventListener('click', function( e ){
            e.preventDefault();
            adWrap.innerHTML = "";
            adWrap.style.width = 0; 
            adWrap.style.height = 0;
        });
    </script>
